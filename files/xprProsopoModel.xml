        <!-- État civil -->
        <xf:bind id="persName" nodeset="xpr:persName"/>
        <xf:bind id="persNameType" nodeset="xpr:persName/@type"/>
        <xf:bind id="forename" nodeset="xpr:persName/xpr:forename"/>
        <xf:bind id="namelink" nodeset="xpr:persName/xpr:namelink"/>
        <xf:bind id="surname" nodeset="xpr:persName/xpr:surname"/>
        
        <xf:bind id="birth" nodeset="xpr:birth"/>
        <xf:bind id="birthDate" nodeset="xpr:birth/xpr:date/@when" type="xf:date"/>
        <xf:bind id="birthPlace" nodeset="xpr:birth/xpr:placeName"/>
        
        <xf:bind id="death" nodeset="xpr:death"/>
        <xf:bind id="deathDate" nodeset="xpr:death/xpr:date/@when" type="xf:date"/>
        <xf:bind id="deathPlace" nodeset="xpr:death/xpr:placeName"/>
        
        <!-- Évènements -->
        <xf:bind id="events" nodeset="xpr:events"/>
        <xf:bind id="event" nodeset="xpr:events/xpr:event"/>
        <xf:bind id="eventType" nodeset="xpr:events/xpr:event/@type"/>
        <xf:bind id="eventLabel" nodeset="xpr:events/xpr:event/xpr:label"/>
        <xf:bind id="eventDesc" nodeset="xpr:events/xpr:event/xpr:desc"/>
        <xf:bind id="eventDate" nodeset="xpr:events/xpr:event/xpr:date" type="xf:date"/>
        <xf:bind id="eventPlaceName" nodeset="xpr:events/xpr:event/xpr:placeName"/>
        <xf:bind id="eventRef" nodeset="xpr:events/xpr:event/xpr:ref"/>
        <xf:bind id="eventBibl" nodeset="xpr:events/xpr:event/xpr:bibl"/>
</xf:model>
